{
    "version": 2,
    "builds": [
        {
            "src": "server.mjs",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "handle": "resource"
        },
        {
            "src": "/(.*)",
            "dest": "/server.mjs"
        }
    ],

    "devCommand": "node server.mjs"

}